{
  "name": "nodecap",
  "version": "6.2.0",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start-ios": "react-native run-ios",
    "start-ios:xs": "react-native run-ios --simulator=\"iPhone XS\"",
    "start-ios:xs-max": "react-native run-ios --simulator=\"iPhone XS Max\"",
    "start-android": "react-native run-android",
    "build-android": "cd android && ./gradlew assembleRelease && cp ./app/build/outputs/apk/release/app-release.apk $HOME/Desktop",
    "build-android:staging": "cd android && ENVFILE=.env.staging ./gradlew assembleRelease && cp ./app/build/outputs/apk/release/app-release.apk $HOME/Desktop",
    "test": "jest",
    "lint": "eslint --ext .js app",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "codepush:android": "code-push release-react hotnode-android android -d Production",
    "codepush:android:staging": "code-push release-react hotnode-android android -d Staging",
    "codepush:ios": "code-push release-react hotnode-ios ios -d Production -m",
    "codepush:ios:staging": "code-push release-react hotnode-ios ios -d Staging -m"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^1.1.2",
    "@nart/react-native-swiper": "^1.5.13",
    "accounting": "^0.4.1",
    "antd-mobile": "2.1.11",
    "axios": "^0.18.0",
    "base-64": "^0.1.0",
    "bignumber.js": "^7.2.1",
    "dva-core": "^1.4.0",
    "dva-loading": "^2.0.5",
    "jcore-react-native": "^1.2.10",
    "jpush-react-native": "^2.2.9",
    "moment": "^2.22.2",
    "ramda": "^0.25.0",
    "rc-form": "2.2.1",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-animatable": "^1.3.0",
    "react-native-camera": "^1.2.0",
    "react-native-code-push": "^5.4.2",
    "react-native-code-push-saga": "^1.0.1",
    "react-native-communications": "^2.2.1",
    "react-native-config": "^0.11.5",
    "react-native-datepicker": "^1.7.2",
    "react-native-device-info": "^0.22.5",
    "react-native-drawer-layout": "^2.0.0",
    "react-native-easy-grid": "^0.2.0",
    "react-native-elements": "^0.19.1",
    "react-native-exit-app": "^1.0.0",
    "react-native-global-props": "^1.1.3",
    "react-native-image-picker": "^0.26.10",
    "react-native-iphone-x-helper": "^1.1.0",
    "react-native-keyboard-aware-scroll-view": "^0.7.2",
    "react-native-keychain": "^3.0.0",
    "react-native-linear-gradient": "^2.4.2",
    "react-native-modal": "^6.5.0",
    "react-native-modal-dropdown": "^0.6.2",
    "react-native-offline": "^3.12.0",
    "react-native-orientation": "^3.1.3",
    "react-native-parallax-scroll-view": "^0.21.3",
    "react-native-pdf": "^5.0.5",
    "react-native-picker-select": "^4.3.0",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-progress": "^3.5.0",
    "react-native-read-more-text": "^1.1.0",
    "react-native-scrollable-tab-view": "^0.8.0",
    "react-native-sentry": "^0.39.0",
    "react-native-shimmer": "^0.3.2",
    "react-native-splash-screen": "^3.1.1",
    "react-native-svg": "6.5.2",
    "react-native-swiper": "^1.5.13",
    "react-native-tab-view": "^1.0.2",
    "react-native-vector-icons": "^5.0.0",
    "react-native-view-shot": "^2.4.0",
    "react-native-wechat": "^1.9.10",
    "react-navigation": "2.2.5",
    "react-navigation-redux-helpers": "^1.1.2",
    "react-redux": "^5.0.7",
    "realm": "^2.15.3",
    "recompose": "^0.30.0",
    "redux-persist": "4.10.2",
    "rn-fetch-blob": "^0.10.13",
    "rn-placeholder": "^1.2.0",
    "victory-native": "30.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-jest": "23.0.0",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.0.1",
    "husky": "^0.14.3",
    "jest": "23.0.0",
    "lint-staged": "^7.1.3",
    "react-test-renderer": "16.3.1"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
}
