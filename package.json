    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.0.1",
    "husky": "^0.14.3",
    "jest": "23.0.0",
    "lint-staged": "^7.1.3",
    "react-test-renderer": "16.3.1"
