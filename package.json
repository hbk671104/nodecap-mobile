    "babel-eslint": "^8.2.3",
    "babel-jest": "23.0.0",
    "babel-plugin-import": "^1.7.0",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "^4.14.0",
