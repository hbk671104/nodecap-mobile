    "rc-form": "^2.2.0",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-code-push": "^5.3.4",
    "react-native-config": "^0.11.5",
    "react-native-elements": "^0.19.1",
    "react-native-iphone-x-helper": "^1.0.3",
    "react-native-linear-gradient": "^2.4.0",
